<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sunnyvinay.overstats">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_ic"
        android:label="OverSurge"
        android:roundIcon="@mipmap/app_ic_round"
        android:supportsRtl="true"
        android:theme="@style/Shock"
        android:versionCode="7"
        android:versionName="1.3.1">
        <activity android:name=".LeagueDisplayActivity"></activity>
        <activity android:name=".OWLActivity" />
        <activity android:name=".PatchActivity" />
        <activity android:name=".PlayerActivity" />
        <activity android:name=".NewsActivity" />
        <activity
            android:name=".MainActivity"
            android:label="OverSurge">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>